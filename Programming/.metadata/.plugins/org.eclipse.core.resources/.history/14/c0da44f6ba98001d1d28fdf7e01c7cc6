package com.project.java;

/**
 * Hello world!
 *
 */
public class App 
{
	
    public static void main( String[] args )
    {
       int count = 1;
       String s = "tata";
       char [] c = s.toCharArray();
       for(int i=0;i<=s.length()-1;i++) {
    	  for(int j = i+1;j<=s.length();j++ ) {
    		  if(c[i] == c[j]) {
    			  
    			  count++;
    			  System.out.println(c[i]+ count);
    		  }
    		  
    	  }
    	   
       }
      
        
    }
}
