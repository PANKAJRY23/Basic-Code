package com.project.java;

/**
 * Hello world!
 *
 */
public class App 
{
	String s = "tata"
    public static void main( String[] args )
    {
       int count = 0;
       for(int i=0;i<=s.length();i++) {
    	   if(s.charAt(i) != '') {
    		   count++;
    	   }
    	   
       }
      
        
    }
}
