package com.project.java;

/**
 * Hello world!
 *
 */
public class App 
{
	
    public static void main( String[] args )
    {
       int count = 0;
       String s = "tata";
       for(int i=0;i<=s.length()-1;i++) {
    	  for(int j = i+1;j<=s.length();j++ ) {
    		  char [] c = s.toCharArray();
    		  System.out.println(c);
    	  }
    	   
       }
      
        
    }
}
